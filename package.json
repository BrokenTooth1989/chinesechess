{
  "dependencies": {
    "protobufjs": "^7.2.5",
    "protobufjs-cli": "^1.1.1"
  },
  "scripts": {
    "build-proto:pbjs": "pbjs --dependency protobufjs/minimal.js --target static-module --wrap commonjs --out ./assets/proto.js/proto.js ./assets/proto/*.proto",
    "build-proto:pbts": "pbts --main --out ./assets/proto.js/proto.d.ts ./assets/proto.js/*.js"
  }
}
